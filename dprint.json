{
  "lineWidth": 160,
  "useTabs": true,
  "typescript": {
    "quoteStyle": "preferSingle",
    "binaryExpression.operatorPosition": "sameLine"
  },
  "includes": [
    "src/**/*.{ts,tsx,js,jsx}"
  ],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],  
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.88.3.wasm"
  ]
}
